options
  fullscreen

task instru1
  show video instrone
  delay 18000
  clear -1 -2

task instru2
  show video instrtwo
  delay 18000
  clear -1 -2

task inter
  show video taskinter
  delay 22000
  clear -1 -2

task intro
  show video taskintro
  delay 55000
  clear -1 -2

task outro
  show video taskoutro
  delay 54000
  clear -1 -2

task visual
  table visualattention
  keys l a
  show bitmap fixpoint
  delay 200
  clear 1
  show bitmap @2 @3 0
  readkey @4 2000
  clear 2
  if STATUS != CORRECT
    show bitmap negativefeedbacksmiley
    delay 1200
    clear 3
  fi
  if STATUS = CORRECT
    show bitmap positivefeedbacksmiley
    delay 1200
    clear 3
  fi
  delay 500
  save @1 STATUS RT BLOCKNAME

block intro
  tasklist
    intro 1
  end

block instruction1
  tasklist
    instru1 1
  end

block training
  message instruction1
  tasklist
    visual 8
  end

block instruction2
  tasklist 
    instru2 1
  end

block real1
  message instruction1
  tasklist
    visual 25
  end

block inter
  tasklist
    inter 1
  end

block instruction3
  tasklist 
    instru2 1
  end

block real2
  message instruction2
  tasklist
    visual 25
  end

block outro
  tasklist
    outro 1
  end
